   "source": [
    "numero = int(input('Digite um número: '))\n",
    "fatorial = numero\n",
    "contador = 1\n",
    "while (numero-contador)>1:\n",
    "    fatorial = fatorial*(numero-contador)\n",
    "    contador += 1\n",
    "print('{0}! = {1}'.format(numero, fatorial))"
